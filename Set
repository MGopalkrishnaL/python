a = {0,1,2,3,4,5,6,7}
 
b = {5,6,7,8,9,10}
print(a | b)
# joined and remove commons
print("\n \n")
print(a & b)
# print commons only
print("\n \n")
print(a - b)
# remove mutual of b from a and print a
print("\n \n")
print(b - a)
# remove mutual of a from b and print b
print("\n \n")
print(a ^ b)
#joined, remove commons and print different
